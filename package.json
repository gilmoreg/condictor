    "babel-core": "6.14.0",
    "babel-eslint": "^7.2.2",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.3",
    "dataloader": "^1.3.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "express": "^4.15.2",
    "express-graphql": "^0.6.4",
    "express-session": "^1.15.1",
    "graphql": "^0.9.3",
    "mongoose": "^4.9.4",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-watch": "^2.0.6",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "faker": "^4.1.0",
    "jest": "^19.0.2",
    "nock": "^9.0.13",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "babel-watch server.js --inspect",
    "client": "cd client && npm start",
    "test-client": "cd client && npm test",
    "test-server": "jest --verbose --runInBand --watch",
    "test-server-debug": "node --debug-brk ./node_modules/.bin/jest --verbose --runInBand --watch",
    "seed": "babel-watch ./seed/seed.js"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "__tests__/.*\\.(js|jsx)$"
  }
}
